{
  "publishConfig": {
    "tag": "next"
  },
  "name": "bootprint-unit-testing",
  "version": "2.2.1",
  "description": "Unit-testing-tools for bootprint-projects",
  "repository": {
    "type": "git",
    "url": "git@github.com:bootprint/bootprint-unit-testing.git"
  },
  "homepage": "https://github.com/bootprint/bootprint-unit-testing",
  "author": {
    "name": "Nils Knappmeier",
    "email": "npm@knappi.org",
    "url": "https://github.com/nknapp"
  },
  "bugs": {
    "url": "https://github.com/bootprint/bootprint-unit-testing/issues"
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --fix .",
    "test": "mocha && npm run lint",
    "coverage": "rm coverage -rf && istanbul cover --dir coverage/mocha ./node_modules/.bin/_mocha --print none",
    "postcoverage": "istanbul report --root coverage text-summary html  && istanbul check-coverage --statements 100",
    "thought": "thought run -a",
    "version": "thoughtful changelog -o -a && npm run thought"
  },
  "dependencies": {
    "bootprint": "^2.0.0",
    "cheerio": "^0.22.0",
    "mkdirp": "^0.5.1",
    "pify": "^2.3.0",
    "rimraf": "^2.6.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "dirty-chai": "^1.2.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "recursive-copy": "^2.0.6",
    "thought": "^1.1.2",
    "thought-plugin-jsdoc": "^1.0.0",
    "thoughtful-release": "^0.3.1",
    "trace-and-clarify-if-possible": "^1.0.0"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "index.js",
    "lib",
    "bin"
  ],
  "keywords": []
}
